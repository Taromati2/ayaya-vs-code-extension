{
    "name": "ayaya",
    "displayName": "ayaya",
    "description": "ayaya SHIORI support",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "aya",
            "aliases": ["yaya", "aya"],
            "extensions": [".dic", ".aym"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "aya",
            "scopeName": "source.ayaya",
            "path": "./syntaxes/aya.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Taromati2/ayaya-vs-code-extension.git"
    },
    "publisher": "steve02081504"
}